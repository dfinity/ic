type RecordError = variant {
    NotAuthorized;
    InvalidHash : text;
    AlreadyExists;
    HashMismatch : record { actual : text; expected : text };
};
type RecordRequest = record { data : blob; hash : text };
type Result = variant { Ok : text; Err : RecordError };
service : () -> {
    greet : (text) -> (text) query;
    "record" : (RecordRequest) -> (Result);
}
