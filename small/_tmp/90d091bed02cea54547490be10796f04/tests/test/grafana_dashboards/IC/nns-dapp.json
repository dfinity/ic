      "targets": [
        {
          "database": "default",
          "datasource": {
            "type": "vertamedia-clickhouse-datasource",
            "uid": "PABAB0CF1FFFED704"
          },
          "dateColDataType": "",
          "dateLoading": false,
          "dateTimeColDataType": "date",
          "dateTimeType": "DATETIME",
          "datetimeLoading": false,
          "extrapolate": true,
          "format": "time_series",
          "formattedQuery": "SELECT $timeSeries as t, count() FROM $table WHERE $timeFilter GROUP BY t ORDER BY t",
          "intervalFactor": 1,
          "query": "WITH\n    'https://nns.ic0.app' AS ORIGIN,\n    ('get_account') AS RELEVANT_ACTION,\n    page_load_ips as\n    (\n    SELECT DISTINCT toUInt32(toStartOfHour(date)) * 1000 as t, remote_addr_hashed\n    FROM $table\n    WHERE $timeFilter\n    AND http_origin == ORIGIN\n    AND ic_method_name == 'http_request'\n    GROUP BY t, remote_addr_hashed\n    ),\n    action_ips as\n    (\n    SELECT DISTINCT toUInt32(toStartOfHour(date)) * 1000 as t, remote_addr_hashed\n    FROM $table\n    WHERE $timeFilter\n    AND http_origin == ORIGIN\n    AND ic_method_name IN RELEVANT_ACTION\n    GROUP BY t, remote_addr_hashed\n    )\nSELECT t, num_no_action / num_page_loads as bounce_rate\nFROM (\n         \n         SELECT t, count(DISTINCT remote_addr_hashed) as num_no_action\n         FROM page_load_ips LEFT JOIN action_ips ON page_load_ips.remote_addr_hashed == action_ips.remote_addr_hashed\n         WHERE action_ips.remote_addr_hashed == ''\n         GROUP BY t\n     ) as no_action_count  INNER JOIN\n     (\n         \n         SELECT t, count(DISTINCT remote_addr_hashed) as num_page_loads\n         FROM\n             (\n                 SELECT t, remote_addr_hashed\n                 FROM action_ips\n                 UNION DISTINCT\n                 SELECT t, remote_addr_hashed\n                 FROM page_load_ips\n             )\n         GROUP BY t\n     ) as all_count ON no_action_count.t == all_count.t\nORDER BY t",
          "rawQuery": "WITH\n    'https://nns.ic0.app' AS ORIGIN,\n    ('get_account') AS RELEVANT_ACTION,\n    page_load_ips as\n    (\n    SELECT DISTINCT toUInt32(toStartOfHour(date)) * 1000 as t, remote_addr_hashed\n    FROM default.http_access_nns_dapp\n    WHERE date >= toDateTime(1690817010) AND date <= toDateTime(1691421810)\n    AND http_origin == ORIGIN\n    AND ic_method_name == 'http_request'\n    GROUP BY t, remote_addr_hashed\n    ),\n    action_ips as\n    (\n    SELECT DISTINCT toUInt32(toStartOfHour(date)) * 1000 as t, remote_addr_hashed\n    FROM default.http_access_nns_dapp\n    WHERE date >= toDateTime(1690817010) AND date <= toDateTime(1691421810)\n    AND http_origin == ORIGIN\n    AND ic_method_name IN RELEVANT_ACTION\n    GROUP BY t, remote_addr_hashed\n    )\nSELECT t, num_no_action / num_page_loads as bounce_rate\nFROM (\n         \n         SELECT t, count(DISTINCT remote_addr_hashed) as num_no_action\n         FROM page_load_ips LEFT JOIN action_ips ON page_load_ips.remote_addr_hashed == action_ips.remote_addr_hashed\n         WHERE action_ips.remote_addr_hashed == ''\n         GROUP BY t\n     ) as no_action_count  INNER JOIN\n     (\n         \n         SELECT t, count(DISTINCT remote_addr_hashed) as num_page_loads\n         FROM\n             (\n                 SELECT t, remote_addr_hashed\n                 FROM action_ips\n                 UNION DISTINCT\n                 SELECT t, remote_addr_hashed\n                 FROM page_load_ips\n             )\n         GROUP BY t\n     ) as all_count ON no_action_count.t == all_count.t\nORDER BY t",
          "refId": "A",
          "round": "0s",
          "skip_comments": true,
          "table": "http_access_nns_dapp",
          "tableLoading": false
        }
      ],
      "title": "Bounce rate",
      "type": "timeseries"
    },
