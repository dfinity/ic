  "scripts": {
    "clean": "rm -fr dist dist-dev dist-prod",
    "build": "rm -fr dist dist-prod && NODE_OPTIONS=--openssl-legacy-provider webpack && mv dist dist-prod",
    "build-dev": "rm -fr dist dist-dev && NODE_OPTIONS=--openssl-legacy-provider FORCE_FETCH_ROOT_KEY=1 webpack --mode development && mv dist dist-dev",
    "start": "NODE_OPTIONS=--openssl-legacy-provider npm run build-dev && http-server dist-dev --proxy https://ic0.app/",
    "lint": "./node_modules/.bin/eslint **/*.ts",
    "lint:fix": "./node_modules/.bin/eslint --fix **/*.ts",
    "publish:release": "",
    "test:coverage": "jest --verbose --collectCoverage",
    "test": "jest",
    "prettier:check": "npx -p prettier -p pretty-quick pretty-quick --check",
    "prettier:format": "npx -p prettier -p pretty-quick pretty-quick",
    "format:check": "npm run prettier:check",
    "format:fix": "npm run prettier:format",
    "format": "npm run prettier:format"
  },
  "engines": {
    "node": "^17",
