[Unit]
Description=Restore keys after in place redeployment
DefaultDependencies=no
After=setup-shared-crypto.service
Before=var-lib-ic-crypto.mount
Before=systemd-fsck@dev-mapper-store\x2dshared\x2d\x2dcrypto.service

[Install]
WantedBy=local-fs.target

[Service]
Type=oneshot
RemainAfterExit=true
ExecStart=/opt/ic/bin/redeploy-restore.sh
